{
  "expo": {
    "name": "AssetManagement",
    "slug": "assetmanagement",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["INTERNET","CAMERA", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.prasunbairagi.AssetManagement"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "03241729-59e3-4497-97ee-0bdd1dabd510"
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow AssetManagement to access your camera",
          "microphonePermission": "Allow AssetManagement to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "owner": "prasunbairagi"
  }
}
